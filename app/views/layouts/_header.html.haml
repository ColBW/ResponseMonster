%header.navbar.navbar-fixed-top
  .navbar-inner
    .container
      = link_to "Response Monster", root_path, id: "logo"
      %nav
        %ul.nav.pull-right
          %li= link_to "Home", root_path
          %li= link_to "Help", '#'
          - if signed_in?
            %li= link_to "Sign Out", signout_path, method: "delete"
          - else
            %li= link_to "Sign In", signin_path